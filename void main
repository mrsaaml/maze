public static void main(String[] args) {
        Maze maze = new Maze(15, 15);
        maze.generateMaze();
        System.out.println("Generated Maze:");
        maze.printMaze();

        if (maze.solveMaze()) {
            System.out.println("\nSolved Maze:");
            maze.printMaze();
        } else {
            System.out.println("No solution found!");
        }
    }
